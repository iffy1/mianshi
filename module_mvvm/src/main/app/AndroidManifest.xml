<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iffy.module_mvvm">

    <application
        android:name="com.iffy.module_base.BaseApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:theme="@style/MyThemeRed" android:name="com.iffy.module_mvvm.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:theme="@style/MyThemeRed" android:name="com.iffy.module_mvvm.oneWayDataBinding.view.MainOneWayActivity">

        </activity>
        <activity android:theme="@style/MyThemeRed" android:name="com.iffy.module_mvvm.oneWayDataBinding.view.MainOneWayBActivity"/>
        <activity android:theme="@style/MyThemeRed" android:name="com.iffy.module_mvvm.twoWayDataBinding.view.MainTwoWayActivity"/>
        <activity android:theme="@style/MyThemeRed" android:name=".mvvm.ui.MVVMActivity"/>
    </application>

</manifest>