        <activity android:theme="@style/MyThemeRed" android:name=".surfaceView.SurfaceActivity" />
        <activity android:theme="@style/MyThemeRed" android:name=".viewCustomize.CustomizeViewActivity" />
        <activity android:theme="@style/MyThemeRed" android:name=".viewEvent.ViewClickActivity" />
        <activity android:theme="@style/MyThemeRed" android:name=".viewOptimiz.ViewOptActivity" />
