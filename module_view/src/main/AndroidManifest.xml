<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iffy.module_view">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".listView.ListActivity"/>
        <activity android:name=".recyclerView.animation.RecyclerAnimationActivity"/>
        <activity android:name=".recyclerView.RecyclerActivity"/>
        <activity android:name=".recyclerView.RecyclerInRecyclerActivity"/>
        <activity android:name=".surfaceView.SurfaceActivity"/>
        <activity android:name=".viewCustomize.CustomizeViewActivity"/>
        <activity android:name=".viewEvent.ViewClickActivity"/>
        <activity android:name=".viewOptimiz.ViewOptActivity"/>
        <activity android:name=".viewRefresh.ViewActivity"/>



    </application>

</manifest>