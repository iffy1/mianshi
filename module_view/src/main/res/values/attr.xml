<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--自定義view的屬性 文字內容-->
    <declare-styleable name="ColorChangeTextView">
        <attr name="wenzi" format="string"/>
        <attr name="daxiao" format="float"/>
        <attr name="yanse" format="color"/>
    </declare-styleable>

    <!--自定义View(MyDispatchView)的属性-->
    <declare-styleable name="MyView">
        <attr name="my_text_color" format="color"/>
        <attr name="my_text_size" format="dimension"/>
    </declare-styleable>

    <!--自定义View(CircleProgress)的属性-->
    <declare-styleable name="CircleProgress">
        <attr name="foreground_color" format="color"/>
        <attr name="background_color" format="color"/>
        <attr name="text_color" format="color"/>
        <attr name="stroke_width" format="dimension"/>
        <attr name="enum_attr" format="enum">
            <enum name="attr_1" value="0"/>
            <enum name="attr_2" value="1"/>
        </attr>
    </declare-styleable>


</resources>
