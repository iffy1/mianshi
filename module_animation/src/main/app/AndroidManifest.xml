<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iffy.module_animation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:theme="@style/MyThemeRed" android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:theme="@style/MyThemeRed" android:name=".propertyAnimation.Interpolator.InterpolatorActivity"/>
        <activity android:theme="@style/MyThemeRed" android:name=".sceneTransition.Aactivity"/>
        <activity android:theme="@style/MyThemeRed" android:name=".sceneTransition.Bactivity"/>
        <activity android:theme="@style/MyThemeRed" android:name=".transition.SimpleTransitionDemoActivity"/>
        <activity android:theme="@style/MyThemeRed" android:name=".propertyAnimation.ValueAnimatorActivity"/>
        <activity android:theme="@style/MyThemeRed" android:name=".propertyAnimation.ObjectAnimatorActivity"/>
    </application>

</manifest>