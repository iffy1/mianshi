<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.iffy.providervisitor">
<!-- Permission Denial: writing com.iffy.mianshi.storage.provider.IffyContentProvider uri
content://com.iffy.mianshi.storage.provider.PersonContentProvider/person from pid=29095,
uid=10405 requires com.iffy.permission.WRITE_CONTENT_PROVIDER, or grantUriPermission()

com.iffy.mianshi.storage.provider 设置了写入权限 必须声明这个权限才能访问
-->
    <uses-permission android:name="com.iffy.permission.WRITE_CONTENT_PROVIDER"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>