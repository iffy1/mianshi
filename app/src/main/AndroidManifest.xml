<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        package="com.iffy.mianshi">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <!--使用权限 来获得访问MessageIPCService的权限-->
    <uses-permission android:name="com.iffy.mianshi.multiProcess.permission.REMOTE_SERVICE_PERMISSION" />

    <!--MessageIPCService声明权限，只有Use这个权限的app才能绑定我-->
    <permission
            android:name="com.iffy.mianshi.multiProcess.permission.REMOTE_SERVICE_PERMISSION"
            android:protectionLevel="normal" />

    <application
            android:name=".application.MyApplication"
            android:allowBackup="false"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            tools:ignore="GoogleAppIndexingWarning">

        <activity android:name=".activity与fragment通信.MainActivity">

        </activity>
        <activity android:name=".application.ActivityApplicationA">

        </activity>
        <activity android:name=".application.ActivityApplicationB"></activity>

        <service android:name=".bindService.Beband" />
        <service android:name=".messenger.MessengerService" />
        <service android:name=".intentservice.MyIntentService" />
        <!--对于webview，图库等，由于存在内存系统泄露或者占用内存过多的问题，
        我们可以采用单独的进程。微信当前也会把它们放在单独的tools进程中
        开启多进程的方法很简单，只需要给四大组件指定android:process标签。-->
        <service
                android:name=".multiProcess.MessageIPCService"
                android:enabled="true"
                android:exported="true"
                android:process=":remote" />

        <activity android:name=".multiProcess.IPCActivity">

        </activity>

        <activity android:name=".oom.OomActivity">

        </activity>
        <activity android:name=".oom.LeakTestActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".intentservice.MyIntentServiceActivity">

        </activity>

        <activity android:name=".messenger.MessengerActivity">

        </activity>
        <activity android:name=".broadcast.BroadcastActivity"></activity>
        <activity android:name=".alertpopupdialog.PopupDialog"></activity>
        <!--动画-->
        <activity android:name=".animator.ValueAnimatorActivity"></activity>

        <activity android:name=".animator.PropertyAnimatorActivity"></activity>
        <activity android:name=".animator.transition.SimpleTransitionDemoActivity"></activity>

        <activity android:name=".animator.sceneTransition.Aactivity"></activity>
        <activity android:name=".animator.sceneTransition.Bactivity">

        </activity>
        <!--布局-->
        <activity android:name=".constraintLayout.MyConstraintLayout">

        </activity>

        <!--Surface view-->
        <activity android:name=".surfaceView.SurfaceActivity"></activity>

        <!--ListActivity-->

        <activity android:name=".list.ListActivity"></activity>

        <!--RecyclerActivity-->
        <activity android:name=".recyclerView.RecyclerActivity"></activity>
        <activity android:name=".recyclerView.animation.RecyclerAnimationActivity">

        </activity>


        <!--RecyclerInRecyclerActivity-->
        <activity android:name=".recyclerView.RecyclerInRecyclerActivity"></activity>

        <!--序列化-->
        <activity android:name=".序列化.ActivityA"></activity>

        <activity android:name=".序列化.ActivityB"></activity>

        <!--差值器-->
        <activity android:name=".animator.插值器.插值器Activity">

        </activity>

        <!--存储-->
        <activity android:name=".存储.StoreActivity"></activity>

        <!--View机制-->
        <activity android:name=".view刷新机制.ViewActivity"></activity>

        <!--Network类-->
        <activity android:name=".network.NetWorkActivity"></activity>

        <!--View优化-->
        <activity android:name=".view优化.ViewOptActivity" />

        <!--自定义View-->
        <activity android:name=".viewCustomize.CustomizeViewActivity">

        </activity>

        <activity android:name=".firebase.MyFireBase">


        </activity>


        <activity android:name=".view事件分发.ViewClickActivity">


        </activity>
        <!-- Aysnc task-->
        <activity android:name=".async.AsyncActivity">

        </activity>
        <activity android:name=".async.CoroutineMainScopeActivity"></activity>
        <activity android:name=".async.RxjavaActivity" />

        <activity android:name=".async.HandlerActivity"></activity>

        <activity android:name=".async.HandlerThreadActivity">

        </activity>


        <activity android:name=".async.HandlerBetweenMainAndSubActivity">

        </activity>

        <!--popupwindow-->
        <activity android:name="com.iffy.mianshi.popupwindow.PopUpWinActivity" />

        <activity android:name=".hashmapSpareArray.HashMapSpareArrayActivity">

        </activity>

        <activity android:name=".多线程.MultithreadingActivity">

        </activity>

        <activity android:name=".多线程.ProduceCustomerActivity">

        </activity>


    </application>

</manifest>