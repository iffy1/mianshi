<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iffy.async">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:theme="@style/MyThemeRed" android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:theme="@style/MyThemeRed" android:name=".Coroutine.CoroutineMainScopeActivity"/>
        <activity android:theme="@style/MyThemeRed" android:name=".handler.HandlerActivity"/>
        <activity android:theme="@style/MyThemeRed" android:name=".handler.HandlerBetweenMainAndSubActivity"/>
        <activity android:theme="@style/MyThemeRed" android:name=".handlerThread.HandlerThreadActivity" />
        <activity android:theme="@style/MyThemeRed" android:name=".handlerThread.HandlerThreadBActivity"/>
        <activity android:theme="@style/MyThemeRed" android:name=".rxjava.RxjavaActivity"/>
        <activity android:theme="@style/MyThemeRed" android:name=".synchronize.SynchronizedActivity"/>
        <activity android:theme="@style/MyThemeRed" android:name=".AsyncActivity"/>
        <activity android:theme="@style/MyThemeRed" android:name=".ProduceCustomerActivity"/>
        <activity android:theme="@style/MyThemeRed" android:name=".intentservice.MyIntentServiceActivity"/>
        <service android:name="com.iffy.async.intentservice.MyIntentService" />



    </application>

</manifest>